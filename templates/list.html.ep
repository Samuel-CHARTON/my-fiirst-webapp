% layout 'default';
% title 'list';
<h1>List of Users</h1>
<table>
  <tr>
    <th>
      id
    </th>
    <th>
      name
    </th>
    <th>
      email
    </th>
  </tr>
<% foreach my $tabs (@$tab) { %>
  <tr>
    <td>
      <%== $tabs->{id} %>
    </td>
    <td>
      <%== $tabs->{name} %>
    </td>
    <td>
      <%== $tabs->{email} %>
    </td>
  </tr>
<% } %>
</table>